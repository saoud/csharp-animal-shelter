@{
  Layout = "_Layout";
}

@using AnimalShelter.Models;

<h1>AnimalTypes</h1>

@if (@Model.Count == 0)
{
  <h3>No animal types have been added yet!</h3>
} 

@foreach (AnimalType animalType in Model)
{
  <h3><a href='/animalType/@animalType.Id'>@animalType.Name</a></h3>
}

<p><a href='/'>Back home</a></p>